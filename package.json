{
  "name": "@frontend/smart-analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./dev.local.js",
    "serve": "node ./vendor/@sn/framework/bin/switch.js ./prompt.js ./switch.json && vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix ./src && prettier --write ./src",
    "inspect": "vue-cli-service inspect",
    "app:build": "cross-env BUILD_ENV=lib vue-cli-service build --target lib --name smart-analytics --formats umd-min src/app-build.js",
    "components:add": "git subtree add --prefix src/common-business-components git@github.com:starlight-bd/frontend-library.git common-business-components-5.1.0",
    "components:push": "git subtree push --prefix src/common-business-components git@github.com:starlight-bd/frontend-library.git common-business-components-5.1.0 --squash",
    "components:pull": "git subtree pull --prefix src/common-business-components git@github.com:starlight-bd/frontend-library.git common-business-components-5.1.0",
    "framework:pull": "git subtree pull --prefix vendor/@sn/framework git@github.com:starlight-bd/frontend-library.git framework-v2-5.0.0 --squash",
    "framework:sync": "node ./vendor/@sn/framework/bin/syncFramework.js",
    "dll:pull": "git subtree pull --prefix vendor/@sn/dll git@github.com:starlight-bd/frontend-library.git dll-v1 --squash",
    "dll:push": "git subtree push --prefix vendor/@sn/dll git@github.com:starlight-bd/frontend-library.git dll-v1",
    "deploy:office": "node ./deployToOffice.js"
  },
  "dependencies": {
    "echarts": "5.3.3",
    "frontend-library": "1.2.18",
    "html2canvas": "^1.0.0-rc.7",
    "js-file-download": "0.4.11",
    "jspdf": "^2.3.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "qs": "6.9.4",
    "vue-click-outside": "1.0.7",
    "vue-html2pdf": "^1.8.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "console-table-printer": "^2.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-externals-plugin": "^3.8.0",
    "prettier": "^2.0.4",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
